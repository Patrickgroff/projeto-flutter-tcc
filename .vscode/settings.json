{
	"dart.lineLength": 190,
	"[dart]": {
		"editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
					"source.organizeImports": "explicit"
				},
		"editor.rulers": [
			190
		],
	},
	"files.exclude": {
    "**/*.g.dart": true,
    "**/*.gr.dart": true,
    "**/*.freezed.dart": true,
  },
}
